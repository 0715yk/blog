{
  "env": {
    "browser": true,
    "es2021": true
  },

  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:@next/next/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "tailwindcss"],
  "rules": {
    "react/react-in-jsx-scope": 0,
    "react/prop-types": 0,
    "no-empty": 0,
    "no-undef": 0
  }
}
